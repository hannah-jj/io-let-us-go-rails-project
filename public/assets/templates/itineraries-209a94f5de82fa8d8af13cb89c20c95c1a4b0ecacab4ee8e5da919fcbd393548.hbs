<table class="table table-striped table-hover">
	<tbody class="itineraries-table">
		{{#if itineraries}}
			{{#each itineraries}}
			  <tr class="info">
			    <td>{{attributes.note}}</td>
			    <td align="right"><a class="btn-info" itin={{id}} href="/events/{{attributes.event-id}}/itineraries/{{id}}">view</td>
			  </tr>
			{{/each}}
		{{else}}
			<tr class="no-comment"><td>no itineraries yet</td></tr>
		{{/if}}
	</tbody>
</table>

