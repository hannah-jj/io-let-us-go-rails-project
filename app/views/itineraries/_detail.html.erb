<!-- shows detail of each itineraries -->
<li>
	<div class="dateline"><%= dateline[:date] %></div>
	<div class="bullet primary"></div>
	
<li>
		
<% dateline[:data].each do |itinerary| %>
	<li class="itinerary">
	<div class="bullet secondary"></div>
	<div class="time"><%= itinerary[:meet_time] %></div>
	<div class="desc">
		<h3><%= itinerary[:note] %></h3>
		<h4>@<%= itinerary[:location] %></h4>
	</div>
	<div class="actions"><%= button_to "Edit", edit_itinerary_path(itinerary), :method => "get", :class =>"btn btn-xs btn-primary"%>
		<%= link_to "Delete", itinerary_path(itinerary), :confirm => "Are you sure?", :method => "delete", :class =>"btn btn-xs btn-primary"%></div>
	</li>
<% end %>
	

