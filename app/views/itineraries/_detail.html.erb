<!-- shows detail of each itineraries -->
<li>
	<div class="dateline"><%= dateline[:date] %></div>
	<div class="bullet primary"></div>
	
<li>
		
<% dateline[:data].each do |itinerary| %>
	<li>
	<div class="bullet secondary"></div>
	<div class="time"><%= itinerary[:meet_time] %></div>
	<div class="desc">
		<h3><%= itinerary[:note] %></h3>
		<h4>@<%= itinerary[:location] %></h4>
	</div>
	</li>
<% end %>
	

