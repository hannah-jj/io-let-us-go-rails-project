<div class="container">
	<nav class = "navbar navbar-inverse">
		<div class="container-fluid">
			<div class="navbar-header">
				<% if user_signed_in? %>
				  <%= link_to "Home", user_path(current_user), :class => "navbar-brand"%>
				  <%= link_to "Edit Profile", edit_user_path(current_user), :class => "navbar-brand"%>
				   <%= link_to "All Events", events_path, :class => "navbar-brand"%>
				  <%= link_to "Add Event", new_event_path, :class => "navbar-brand"%>
				  <%= link_to "Sign out", destroy_user_session_path, method: :delete , :class => "navbar-brand"%>
				<% else %>
					<%= link_to "Home", events_path, :class => "navbar-brand"%>
				  <%= link_to "Sign in", new_user_session_path, :class => "navbar-brand"%>
				  <%= link_to "Sign up", new_user_registration_path, :class => "navbar-brand" %>
				<% end %>
			</div>
		</div>
	</nav>
</div>